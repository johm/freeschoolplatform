<p id="notice"><%= notice %></p>

<h1>Course Course Series</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Course series</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_course_series.each do |course_course_series| %>
      <tr>
        <td><%= course_course_series.course %></td>
        <td><%= course_course_series.course_series %></td>
        <td><%= link_to 'Show', course_course_series %></td>
        <td><%= link_to 'Edit', edit_course_course_series_path(course_course_series) %></td>
        <td><%= link_to 'Destroy', course_course_series, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course Course Series', new_course_course_series_path %>
